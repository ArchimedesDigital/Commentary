function backgroundImages() {
	$('.background-image-holder').each(function (i, elem) {
		var imgSrc = $(elem).children('img').attr('src');
		$(elem).css('background', /* istanbul ignore next */'url("' + imgSrc + '")');
		$(elem).children('img').hide();
		$(elem).css('background-position', 'initial');
		$(elem).addClass('fadeIn');
	});

	// Fade in background images
	setTimeout(function () {
		$('.remove-blur').removeClass('blur');
		$('.remove-blur').removeClass('blur-10').addClass('blur');
	}, 300);
